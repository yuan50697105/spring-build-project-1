<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>spring-repositories-mysql</artifactId>
        <groupId>org.example.spring</groupId>
        <version>0.0.39-M6-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>spring-repositories-mysql_2</artifactId>

    <dependencies>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-auth_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-bill_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-customer_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-department_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-items_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-patient_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.example.spring</groupId>
            <artifactId>spring-repositories-mysql-report_2</artifactId>
            <version>0.0.39-M6-SNAPSHOT</version>
        </dependency>
    </dependencies>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-shade-plugin</artifactId>
                    <version>3.2.4</version>
                    <executions>
                        <execution>
                            <phase>package</phase>
                            <id>shade</id>
                            <goals>
                                <goal>shade</goal>
                            </goals>
                            <configuration>
                                <artifactSet>
                                    <includes>
                                        <include>org.example.spring:spring-repositories-mysql-auth_1</include>
                                        <include>org.example.spring:spring-repositories-mysql-bill_1</include>
                                        <include>org.example.spring:spring-repositories-mysql-customer_1</include>
                                        <include>org.example.spring:spring-repositories-mysql-department_1</include>
                                        <include>org.example.spring:spring-repositories-mysql-items_1</include>
                                        <include>org.example.spring:spring-repositories-mysql-report_1</include>
                                    </includes>
                                </artifactSet>
                                <keepDependenciesWithProvidedScope>true</keepDependenciesWithProvidedScope>
                                <createDependencyReducedPom>true</createDependencyReducedPom>
                                <filters>
                                    <filter>
                                        <artifact>*:*</artifact>
                                        <excludes>
                                            <exclude>META-INF/*.SF</exclude>
                                            <exclude>META-INF/*.DSA</exclude>
                                            <exclude>META-INF/*.RSA</exclude>
                                        </excludes>
                                    </filter>
                                </filters>
                                <transformers>
                                    <transformer implementation="org.apache.maven.plugins.shade.resource.AppendingTransformer">
                                        <resource>META-INF/spring.handlers</resource>
                                    </transformer>
                                    <transformer implementation="org.springframework.boot.maven.PropertiesMergingResourceTransformer">
                                        <resource>META-INF/spring.factories</resource>
                                    </transformer>
                                    <transformer implementation="org.apache.maven.plugins.shade.resource.AppendingTransformer">
                                        <resource>META-INF/spring.schemas</resource>
                                    </transformer>
                                    <transformer implementation="org.apache.maven.plugins.shade.resource.ServicesResourceTransformer"/>
                                    <!--                                <transformer implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer">-->
                                    <!--                                    <mainClass>${start-class}</mainClass>-->
                                    <!--                                </transformer>-->
                                </transformers>
                            </configuration>
                        </execution>
                    </executions>
                    <dependencies>
                        <dependency>
                            <groupId>org.springframework.boot</groupId>
                            <artifactId>spring-boot-maven-plugin</artifactId>
                            <version>2.5.1</version>
                        </dependency>
                    </dependencies>
                </plugin>
            </plugins>
        </pluginManagement>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
            </plugin>
        </plugins>
    </build>
</project>